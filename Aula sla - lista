//faca um programa q tenha 4 opções: primeira; cadastrar cliente segunda; remover ultimo cliente terceira;ver listas de clientes e sair da aplicação

let clientes = []
while(true){
    let opção = prompt('O que deseja fazer? \n1. Cadastrar clientes.\n2. Remover o último cliente \n3. Ver clientes \n4. Sair da aplicação')
    if (opção = 1){
        let nome = prompt('Qual o nome do cliente?')
        clientes.push(nome)
        console.log('Usuário cadastrado com sucesso.')
        console.log(clientes)
    }
}
else if(opção == 2){
clientes.pop ()
console.log(clientes)
}
else if(opção == 3){
    console.log(clientes)
}
else if(opção == 4){
    break
}
else{
    console.log ('Opção inválida')
}
